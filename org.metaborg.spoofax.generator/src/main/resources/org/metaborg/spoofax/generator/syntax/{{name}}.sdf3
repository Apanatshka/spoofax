// Grammar for the {{name}} language
// By default, based on an example "entity language" syntax

module {{name}}

imports 

  Common

context-free start-symbols

  {{startSymbol}}

context-free syntax

  {{startSymbol}}.Module = <
    module <ID> 
    
    <{Definition "\n\n"}*>
  >

  Definition.Entity = <
    entity <ID> {
      <{Property "\n"}*>
    }
  >
    
  Property.Property = <<ID> : <Type>>
  
  Type.Type = <<ID>>