module sdf2imp/project/create-build-properties

imports
  sdf2imp/util/-
  sdf2imp/options

strategies

  create-build-properties =
    MavenBuild
  <+
    <file-exists> "build.properties"
  <+
    <output-text-file(|[], "build.properties")>
"source.. = editor/java/
output.. = target/classes/
bin.includes = META-INF/,\\
               plugin.xml,\\
               include/,\\
               icons/,\\
               .
bin.excludes = trans/
"
