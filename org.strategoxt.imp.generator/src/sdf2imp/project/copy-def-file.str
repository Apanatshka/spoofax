module sdf2imp/project/copy-def-file
    
imports
  sdf2imp/util/-
  sdf2imp/services/configure-main-descriptor
  sdf2imp/options

strategies

  copy-def-file =
    if NoExistingDescriptor; InputFile; not(<file-exists> "include") then
      <mkdir> "include";
      <copy-file> (<InputFile>, "include")
    end