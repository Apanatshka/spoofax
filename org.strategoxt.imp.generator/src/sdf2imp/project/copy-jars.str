module sdf2imp/project/copy-jars

imports
  libstratego-lib

strategies
  
  copy-jars =
    if JarLocations then
      verbose-msg(!"Copying jar files", JarLocations; separate-by(|","); concat-strings);
      <file-exists <+ mkdir> "aux/";
      JarLocations;
      list-loop(<copy-file> (<id>, "aux/"))
    end
